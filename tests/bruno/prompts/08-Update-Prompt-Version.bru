meta {
  name: Update Prompt Version
  type: http
  seq: 20
}

patch {
  url: {{baseUrl}}/prompts/{{createdPromptId}}
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  client_id: {{clientId}}
  client_api_key: {{clientApiKey}}
}

body:json {
  {
    "version": 10
  }
}

tests {
  test("Status code is 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response contains updated version", function() {
    const prompt = res.getBody();
    expect(prompt.version).to.equal(10);
  });
}
