tests {
  test("Status code is 401", function() {
    expect(res.getStatus()).to.equal(401);
  });
  
  test("Response contains invalid key error message", function() {
    expect(res.getBody()).to.have.property('detail');
    expect(res.getBody().detail).to.equal('Invalid admin API key');
  });
}



