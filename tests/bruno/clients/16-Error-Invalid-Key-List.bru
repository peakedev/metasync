meta {
  name: Error - Invalid API Key (GET /clients)
  type: http
  seq: 16
}

get {
  url: {{baseUrl}}/clients
  body: none
  auth: none
}

headers {
  Content-Type: application/json
  admin_api_key: invalid-api-key-12345
